#include <omp.h>
#include <stdio.h>
double calc(int count);
int main(){
  int N=10,i;
  double datos[N];
  double tiempo= omp_get_wtime();
#pragma omp parallel
  for(i=0;i<N;i++)
    datos[i]=calc(i);
  tiempo =omp_get_wtime()-tiempo; 
  printf("tiempo: %i",
}

double calc(int count)
{
  int i;
  double d=1.0;
  for(i=0;i<count*count;i++)
	d+=d;
  return d;
}
